{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "firefox",
      "request": "launch",
      "name": "vuejs: firefox",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}/src",
      "pathMappings": [
        {
          "url": "webpack:///components",
          "path": "${workspaceFolder}/components"
        },
        {
          "url": "http://localhost:3000/_nuxt/pages/index.js",
          "path": "${workspaceFolder}/store/actions/index.js"
        },
        {
          "url": "webpack:///database",
          "path": "${workspaceFolder}/database"
        },
        {
          "url": "webpack:///pages",
          "path": "${workspaceFolder}/pages"
        },
        {
          "url": "webpack:///src/",
          "path": "${webRoot}/"
        }
      ]
    }
  ]
}